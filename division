import java.util.Scanner;

class MyClass {
  public static void main(String[] args) {
    int x, y, sum;
    Scanner Obj = new Scanner(System.in);
    System.out.println("Type a number:");
    x = Obj.nextInt();

    System.out.println("Type another number:");
    y = Obj.nextInt();

    div = x/y;
    System.out.println("Division is: " + div);
  }
}

